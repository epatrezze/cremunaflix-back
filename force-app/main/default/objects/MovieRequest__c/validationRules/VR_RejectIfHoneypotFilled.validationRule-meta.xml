<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_RejectIfHoneypotFilled</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(), NOT(ISBLANK(Honeypot__c)))</errorConditionFormula>
    <errorMessage>Invalid submission.</errorMessage>
    <errorDisplayField>Honeypot__c</errorDisplayField>
</ValidationRule>
