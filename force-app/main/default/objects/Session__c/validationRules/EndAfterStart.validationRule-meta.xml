<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EndAfterStart</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(EndDateTime__c)), EndDateTime__c &lt; StartDateTime__c)</errorConditionFormula>
    <errorMessage>End Date/Time must be after Start Date/Time.</errorMessage>
    <errorDisplayField>EndDateTime__c</errorDisplayField>
</ValidationRule>
